# Path to the HunyuanVideo model
HUNYUANVIDEO_MODEL_PATH=/path/to/hunyuanvideo/model

# Path to FP8 weights for memory optimization
FP8_WEIGHTS_PATH=/path/to/hunyuanvideo/fp8_weights.pt

# Directory to store generated videos
RESULTS_DIR=./results

# Maximum number of concurrent generation jobs
MAX_CONCURRENT_JOBS=1

# ngrok authentication token (optional but recommended)
NGROK_AUTH_TOKEN=your_ngrok_auth_token 